{
  "name": "build-week-scaffolding-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "resetdb": "npm run rollback && npm run migrate && npm run seed",
    "migrateh": "heroku run knex migrate:latest -a marketplace-be-02",
    "rollbackh": "heroku run knex migrate:rollback -a marketplace-be-02",
    "databaseh": "heroku pg:psql -a marketplace-be-02",
    "seedh": "heroku run knex seed:run -a marketplace-be-02",
    "resetdbh": "npm run rollbackh && npm run migrateh && npm run seedh",
    "test": "cross-env NODE_ENV=testing jest --verbose --runInBand",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "16.13.2",
    "npm": "6.14.11"
  },
  "license": "ISC",
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "14.3.0",
    "express": "4.17.2",
    "helmet": "5.0.2",
    "knex": "1.0.1",
    "knex-cleaner": "1.3.1",
    "pg": "8.7.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.0",
    "cross-env": "7.0.3",
    "eslint": "8.7.0",
    "jest": "27.4.7",
    "nodemon": "2.0.15",
    "supertest": "6.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominstituteoftechnology/build-week-scaffolding-node.git"
  }
}
